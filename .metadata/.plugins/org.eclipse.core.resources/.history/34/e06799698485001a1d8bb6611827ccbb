package learn;
import java.util.*;
import java.lang.Enum;

public enum Langage {
	  JAVA,
	  C,
	  CPlus,
	  PHP;	
	}

public class A {
	public static void main(String[] args){
		
		for(Langage lang : Langage.values()){
		      if(Langage.JAVA.equals(lang))
		        System.out.println("J'aime le : " + lang);
		      else
		        System.out.println(lang);
		    }
		
			}
}
