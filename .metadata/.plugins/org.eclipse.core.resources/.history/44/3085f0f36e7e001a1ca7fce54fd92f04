package AIAC;

import java.util.*;

public class project {
	public static void main(String[] args) {
		Filiere f1=new Filiere("GI");
		Filiere f2=new Filiere("GEET");
		
		Promo p = new Promo (16);
		
		Class c1=new Class (f1,p);
		Class c2= new Class(f2,p);
		
		Student s=new Student("RHIF","AFAF");
		
		c1.addStud(s);
		
		c1.affichestudClass();		
		f1.afficheClassFiliere();
		f2.afficheClassFiliere();
		p.afficheClassPromo();
		
		
		
	
		
		
		
		
		
		
		
		
}

}
